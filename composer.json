    "type": "library",
    "require": {
        "php": "^7.4|^8",
        "symfony/filesystem": "^5.0",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Dmitriy V Kozubskiy",
            "email": "kozubsky@inbox.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodexSoft\\ComposerLocalPackageUpdater\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
